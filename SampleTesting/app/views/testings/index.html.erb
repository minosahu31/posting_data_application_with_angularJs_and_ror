<!-- <div ng-app="TestingCenter"> -->
<p id="notice"><%= notice %></p>

<h1>Listing Testings</h1>

<table><!-- ng-controller="testingsController"> -->
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>

    <% @testings.each do |testing| %>
      <tr>
        <td><%= testing.title %></td>
        <td><%= testing.description %></td>
        <td><% link_to 'Show', testing_path(testing) %></td>
        <td><%= link_to 'Edit', edit_testing_path(testing) %></td>
        <td><%= link_to 'Destroy', testing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testing', new_testing_path %>
